<span style="display: none;" id="stopTimerLink" data-link="{link RestTestExecution:stopTimer}"></span>
<span style="display: none;" id="startTimerLink" data-link="{link RestTestExecution:startTimer}"></span>
<span style="display: none;" id="getTestedTestStepsLink" data-link="{link RestTestExecution:getTestedTestSteps}"></span>
<span style="display: none;" id="insertTestExecutionLink" data-link="{link RestTestExecution:insertTestExecution}"></span>
<span style="display: none;" id="redrawDatagridLink" data-link="{link redrawDatagrid!}"></span>
<span style="display: none;" id="getTestStepHistoryLink" data-link="{link RestTestExecution:getTestStepHistory}"></span>
<span style="display: none;" id="setForceStatusLink" data-link="{link RestTestExecution:setForceStatus}"></span>
